{
  "name": "personal_site_react",
  "version": "0.0.2",
  "private": true,
  "homepage": "https://phillyfan1138.github.io",
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "js-yaml": "^3.13.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react-scripts": "^3.2.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1",
    "react-ga": "^2.5.6",
    "react-highcharts": "^16.1.0",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^7.1.0",
    "victory": "^31.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --no-semi",
      "git add"
    ]
  },
  "swagger_urls": [
    "https://cdn.jsdelivr.net/gh/phillyfan1138/credit_faas@v4/docs/openapi_merged.yml",
    "https://cdn.jsdelivr.net/gh/phillyfan1138/ops_faas@v5/docs/openapi_merged.yml",
    "https://cdn.jsdelivr.net/gh/phillyfan1138/market_faas@v3/docs/openapi_merged.yml"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "generate_forms": "node ./formGenerator/generateBootstrapForm"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/components/generatedComponents/*",
      "!src/setupTests.js",
      "!src/serviceWorker.js",
      "!src/index.js"
    ]
  }
}
