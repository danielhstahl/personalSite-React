{
  "name": "personal_site_react",
  "version": "0.0.2",
  "private": true,
  "homepage": "https://danielhstahl.github.io",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^1.15.3",
    "prop-types": "^15.7.2",
    "react-scripts": "^4.0.3"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "victory": "^35.10.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --no-semi",
      "git add"
    ]
  },
  "swagger_urls": [
    "https://cdn.jsdelivr.net/gh/danielhstahl/credit_faas@v0.0.1/docs/openapi_merged.yml",
    "https://cdn.jsdelivr.net/gh/danielhstahl/ops_faas@v0.0.1/docs/openapi_merged.yml",
    "https://cdn.jsdelivr.net/gh/danielhstahl/market_faas@v0.0.1/docs/openapi_merged.yml"
  ],
  "pdf_urls": [
    {
      "url": "https://github.com/danielhstahl/CreditRiskPaper/releases/download/0.1.0/main.pdf",
      "name": "CreditRiskPaper"
    },
    {
      "url": "https://github.com/danielhstahl/OpsRiskPaper/releases/download/0.2.0/main.pdf",
      "name": "OpsRiskPaper"
    },
    {
      "url": "https://github.com/danielhstahl/PaperMarketRisk/releases/download/0.1.0/MarketRiskDocumentation.pdf",
      "name": "MarketRiskDocumentation"
    },
    {
      "url": "https://github.com/danielhstahl/CreditRiskExtensions/releases/download/0.1.0/main.pdf",
      "name": "CreditRiskExtensions"
    }
  ],
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "generate_forms": "node ./scripts/generateBootstrapForm",
    "get_pdf": "node ./scripts/downloadPdf"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/components/generatedComponents/*",
      "!src/setupTests.js",
      "!src/serviceWorker.js",
      "!src/index.js",
      "!src/**/*.stories.js"
    ]
  }
}